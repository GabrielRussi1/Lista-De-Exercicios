#include <stdio.h>

int main() {
    int A, B, temp;

    printf("Digite o valor de A: ");
    scanf("%d", &A);
    
    printf("Digite o valor de B: ");
    scanf("%d", &B);

    // Troca usando a variável temporária
    temp = A;
    A = B;
    B = temp;

    // resultado
    printf("Após a troca:\n");
    printf("A = %d\n", A);
    printf("B = %d\n", B);

    return 0;
}
