#include <stdio.h>

int main() {
    float celsius, fahrenheit;

    printf("Digite a temperatura em Celsius: ");
    scanf("%f", &celsius);

    // Cálculo da conversão
    fahrenheit = (celsius * 9 / 5) + 32;

    // resultado
    printf("A temperatura em Fahrenheit é: %.2f°F\n", fahrenheit);

    return 0;
}
